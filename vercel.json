{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://style-sphere-api.vercel.app/"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://style-sphere-ss.vercel.app/" }
  ]
}
